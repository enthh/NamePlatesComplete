<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\..\FrameXML\UI.xsd">
	<Script file="NamePlatesComplete_CastExtra.lua" />

	<Frame name="NamePlatesComplete_CastExtraTemplate" mixin="NamePlatesComplete_CastExtraMixin"
		virtual="true">
		<Size y="14" />
		<Layers>
			<Layer level="BACKGROUND" textureSubLevel="10">
				<Texture parentKey="TextBorder" atlas="ui-castingbar-textbox" hidden="false">
					<!-- <Size x="185" y="14" /> -->
					<Anchors>
						<Anchor point="TOPLEFT" x="0" y="0" />
						<Anchor point="BOTTOMRIGHT" x="0" y="0" />
					</Anchors>
				</Texture>
				<Texture parentKey="InterruptGlow" atlas="cast_interrupt_outerglow" alpha="0.5"
					hidden="true">
				</Texture>
			</Layer>

			<Layer level="OVERLAY">
				<FontString parentKey="Text" inherits="SystemFont_Shadow_Small">
					<Size x="0" y="12" />
					<Anchors>
						<Anchor point="TOPLEFT" x="3" y="-2" />
						<Anchor point="BOTTOMRIGHT" x="-3" y="2" />
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad method="OnLoad" />
			<OnEvent method="OnEvent" />
		</Scripts>
	</Frame>

	<Frame name="NamePlatesComplete_CastExtraDriverFrame" toplevel="true"
		mixin="NamePlatesComplete_CastExtraDriverMixin">
		<Scripts>
			<OnLoad method="OnLoad" />
			<OnEvent method="OnEvent" />
		</Scripts>
	</Frame>

</Ui>